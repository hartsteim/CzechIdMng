  "scripts": {
    "preinstall": "npx npm-force-resolutions",
    "product-install": "npm install && gulp install",
    "modules-link": "mkdir ../node_modules && ln -s ../node_modules",
    "modules-link-or-skip": "mkdir -p ../node_modules && test -L node_modules || ln -s ../node_modules",
    "czechidm-modules-link": "test -L ./czechidm-modules/czechidm-core || ln -s ../../czechidm-core ./czechidm-modules/; test -L ./czechidm-modules/czechidm-acc || ln -s ../../czechidm-acc ./czechidm-modules/; test -L ./czechidm-modules/czechidm-vs || ln -s ../../czechidm-vs ./czechidm-modules/; test -L ./czechidm-modules/czechidm-rpt || ln -s ../../czechidm-rpt ./czechidm-modules/",
    "test": "npm run tests-set",
    "test-lint": "npm run lint && npm run tests-set",
    "tests-set": "mocha ./test/**/*.js ./czechidm-modules/*/test/**/**/*.js --reporter nyan --compilers js:babel/register --recursive --check-leaks --require ./test/setup.js",
    "lint": "eslint ./",
    "test-watch": "mocha --watch ./test/**/*.js ./czechidm-modules/*/test/**/**/*.js --reporter nyan --compilers js:babel/register --recursive --check-leaks --require ./test/setup.js"
  },
  "browser": {
    "jsdom": false
  },
